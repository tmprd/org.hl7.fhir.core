@prefix fhir: <http://hl7.org/fhir/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

# - resource -------------------------------------------------------------------

<http://hl7.org/fhir/OperationOutcome/allok> a fhir:OperationOutcome ;
  fhir:nodeRole fhir:treeRoot ;
  fhir:id [ fhir:v "allok"] ; # 
  fhir:text [
     fhir:status [ fhir:v "generated" ] ;
     fhir:div "<div xmlns=\"http://www.w3.org/1999/xhtml\">\n      <p>All OK</p>\n    </div>"^^rdf:XMLLiteral
  ] ; # 
  fhir:issue ( [
     fhir:severity [ fhir:v "information" ] ;
     fhir:code [ fhir:v "informational" ] ;
     fhir:details [
       fhir:text [ fhir:v "All OK" ]
     ]
  ] ) . # 

