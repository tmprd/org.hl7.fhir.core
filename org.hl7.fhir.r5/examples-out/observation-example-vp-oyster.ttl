       fhir:code [ fhir:v "A" ] ;
       fhir:display [ fhir:v "Abnormal" ]
     ] )
  ] ) ; # 
  fhir:specimen [
     fhir:reference [ fhir:v "#group1" ]
  ] ; # 
  fhir:referenceRange ( [
     fhir:text [ fhir:v "NEGATIVE" ]
  ] ) . # 

