# - resource -------------------------------------------------------------------

#   This example where a patient reports is not taking now but will be taking in future   

<http://hl7.org/fhir/MedicationStatement/example009> a fhir:MedicationStatement ;
  fhir:nodeRole fhir:treeRoot ;
  fhir:id [ fhir:v "example009"] ; # 
  fhir:contained ( [
     a fhir:Medication ;
     fhir:id [ fhir:v "med0315" ] ;
     fhir:code [
       fhir:coding ( [
         fhir:system [ fhir:v "http://hl7.org/fhir/sid/ndc"^^xsd:anyURI ] ;
         fhir:code [ fhir:v "0378-9112-93" ] ;
         fhir:display [ fhir:v "Nitroglycerin, 30 POUCH in 1 CARTON (0378-9112-93) > 1 PATCH in 1 POUCH (0378-9112-16) > 14 h in 1 PATCH (package)" ]
       ] )
     ] ;
     fhir:doseForm [
       fhir:coding ( [
         a sct:376251004 ;
         fhir:system [ fhir:v "http://snomed.info/sct"^^xsd:anyURI ] ;
         fhir:code [ fhir:v "376251004" ] ;
         fhir:display [ fhir:v "Nitroglycerin 200 microgram/hour prolonged-release transdermal patch" ]
       ] )
     ]
  ] ) ; # 
  fhir:partOf ( [
     fhir:link <http://hl7.org/fhir/MedicationStatement/example008> ;
     fhir:reference [ fhir:v "MedicationStatement/example008" ]
  ] ) ; # 
  fhir:status [ fhir:v "recorded"] ; # 
  fhir:medication [
     fhir:reference [
       fhir:reference [ fhir:v "#med0315" ]
     ] #   Linked to a RESOURCE Medication  
  ] ; # 
  fhir:subject [
     fhir:link <http://hl7.org/fhir/Patient/pat1> ;
     fhir:reference [ fhir:v "Patient/pat1" ] ;
     fhir:display [ fhir:v "Donald Duck" ]
  ] ; # 
  fhir:effective [
     a fhir:DateTime ;
     fhir:v "2015-01-23"^^xsd:date
  ] ; # 
  fhir:dateAsserted [ fhir:v "2015-02-22"^^xsd:date] ; # 
  fhir:informationSource ( [
     fhir:link <http://hl7.org/fhir/Patient/pat1> ;
     fhir:reference [ fhir:v "Patient/pat1" ] ;
     fhir:display [ fhir:v "Donald Duck" ]
  ] ) ; # 
  fhir:note ( [
     fhir:text [ fhir:v "Patient reports they used the medication patch on this day." ]
  ] ) . # 

<http://hl7.org/fhir/MedicationStatement/example008> a fhir:MedicationStatement .

<http://hl7.org/fhir/Patient/pat1> a fhir:Patient .

