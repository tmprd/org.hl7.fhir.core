# - resource -------------------------------------------------------------------

<http://hl7.org/fhir/Permission/example-composite> a fhir:Permission ;
  fhir:nodeRole fhir:treeRoot ;
  fhir:id [ fhir:v "example-composite"] ; # 
  fhir:status [ fhir:v "active"] ; # 
  fhir:asserter [
     fhir:link <http://hl7.org/fhir/Organization/f203> ;
     fhir:reference [ fhir:v "Organization/f203" ]
  ] ; # 
  fhir:date ( [ fhir:v "2018-12-24"^^xsd:date] ) ; # 
  fhir:combining [ fhir:v "permit-overrides"] ; # 
  fhir:rule ( [] [
     fhir:type [ fhir:v "permit" ] ;
     fhir:data ( [
       fhir:security ( [
         fhir:system [ fhir:v "http://terminology.hl7.org/CodeSystem/v3-ActCode"^^xsd:anyURI ] ;
         fhir:code [ fhir:v "SUD" ]
       ] )
     ] ) ;
     fhir:activity ( [
       fhir:actor ( [
         fhir:reference [ ]
       ] ) ;
       fhir:action ( [
         fhir:coding ( [
           fhir:system [ fhir:v "http://terminology.hl7.org/CodeSystem/consentaction"^^xsd:anyURI ] ;
           fhir:code [ fhir:v "access" ]
         ] )
       ] ) ;
       fhir:purpose ( [
         fhir:coding ( [
           fhir:system [ fhir:v "http://terminology.hl7.org/CodeSystem/v3-ActReason"^^xsd:anyURI ] ;
           fhir:code [ fhir:v "TREAT" ]
         ] )
       ] )
     ] )
  ] ) . # 

<http://hl7.org/fhir/Organization/f203> a fhir:Organization .

