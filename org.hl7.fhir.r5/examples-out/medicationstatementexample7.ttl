#   This example where a patient reports is not taking now but will be taking in future   

<http://hl7.org/fhir/MedicationStatement/example007> a fhir:MedicationStatement ;
  fhir:nodeRole fhir:treeRoot ;
  fhir:id [ fhir:v "example007"] ; # 
  fhir:contained ( [
     a fhir:Medication ;
     fhir:id [ fhir:v "med0315" ] ;
     fhir:code [
       fhir:coding ( [
         fhir:system [ fhir:v "http://www.nlm.nih.gov/research/umls/rxnorm"^^xsd:anyURI ] ;
         fhir:code [ fhir:v "1797865" ] ;
         fhir:display [ fhir:v "mometasone furoate 0.05 MG/ACTUAT Metered Dose Nasal Spray" ]
       ] )
     ] ;
     fhir:doseForm [
       fhir:coding ( [
         a sct:385157007 ;
         fhir:system [ fhir:v "http://snomed.info/sct"^^xsd:anyURI ] ;
         fhir:code [ fhir:v "385157007" ] ;
         fhir:display [ fhir:v "Nasal Spray" ]
       ] )
     ]
  ] ) ; # 
  fhir:status [ fhir:v "recorded"] ; # 
  fhir:medication [
     fhir:reference [
       fhir:reference [ fhir:v "#med0315" ]
     ] #   Linked to a RESOURCE Medication  
  ] ; # 
  fhir:subject [
     fhir:link <http://hl7.org/fhir/Patient/pat1> ;
     fhir:reference [ fhir:v "Patient/pat1" ] ;
     fhir:display [ fhir:v "Donald Duck" ]
  ] ; # 
  fhir:informationSource ( [
     fhir:link <http://hl7.org/fhir/Patient/pat1> ;
     fhir:reference [ fhir:v "Patient/pat1" ] ;
     fhir:display [ fhir:v "Donald Duck" ]
  ] ) ; # 
  fhir:note ( [
     fhir:text [ fhir:v "patient plans to start using for seasonal allergies in the Spring when pollen is in the air" ]
  ] ) . # 

<http://hl7.org/fhir/Patient/pat1> a fhir:Patient .

