@prefix fhir: <http://hl7.org/fhir/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

# - resource -------------------------------------------------------------------

<http://hl7.org/fhir/OperationOutcome/searchfail> a fhir:OperationOutcome ;
  fhir:nodeRole fhir:treeRoot ;
  fhir:id [ fhir:v "searchfail"] ; # 
  fhir:text [
     fhir:status [ fhir:v "generated" ] ;
     fhir:div [ fhir:v "<div xmlns=\"http://www.w3.org/1999/xhtml\">\n      <p>The \"name\" parameter has the modifier \"exact\" which is not supported by this server</p>\n    </div>"^^rdf:XMLLiteral ]
  ] ; # 
  fhir:issue ( [
     fhir:severity [ fhir:v "fatal" ] ;
     fhir:code [ fhir:v "code-invalid" ] ;
     fhir:details [
       fhir:text [ fhir:v "The \"name\" parameter has the modifier \"exact\" which is not supported by this server" ]
     ] ;
     fhir:location ( [ fhir:v "http.name:exact" ] )
  ] ) . # 

